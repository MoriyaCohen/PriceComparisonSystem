{
    "Storage": {
        "Provider": "Azure",
        "Azure": {
            "ConnectionString": "...",
            "ContainerName": "price-data"
        },
        "Local": {
            "BasePath": "C:/PriceData"
        }
    },
    "Scheduling": {
        "DailyRunTime": "03:00:00",
        "RetryAttempts": 3,
        "RetryDelayMinutes": 5
    },
    "Chains": [
        {
            "Name": "KingStore",
            "Type": "BinaProjects",
            "BaseUrl": "https://kingstore.binaprojects.com",
            "Enabled": true
        },
        {
            "Name": "Shufersal",
            "Type": "Custom",
            "BaseUrl": "https://prices.shufersal.co.il",
            "Enabled": true
        }
    ]
}