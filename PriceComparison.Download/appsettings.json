{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "PriceComparison.Download.Services.BinaProjectsDownloadService": "Debug"
    }
  },
  "BinaProjects": {
    "ConfigFilePath": "BinaProjectsNetworks.json",
    "DownloadFolder": "DownloadedFiles",
    "TimeoutSeconds": 15,
    "RetryCount": 1,
    "RetryDelaySeconds": 5,
    "MaxConcurrentDownloads": 3,
    "MaxConnectionsPerServer": 10,
    "UserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (HTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
    "AcceptLanguage": "he-IL,he;q=0.9,en;q=0.8",
    "UseCookies": false,
    "IgnoreSSLErrors": false,
    "AdditionalHeaders": {
      "Cache-Control": "no-cache",
      "Pragma": "no-cache"
    }
  },
  "Storage": {
    "Provider": "Local",
    "Azure": {
      "ConnectionString": "",
      "ContainerName": "price-data"
    },
    "Local": {
      "BasePath": "DownloadedFiles"
    }
  },
  "Scheduling": {
    "DailyRunTime": "03:00:00",
    "RetryAttempts": 3,
    "RetryDelayMinutes": 5
  }
